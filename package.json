{
  "name": "clean-fetch",
  "version": "0.0.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "repository": "https://github.com/tranvansang/clean-fetch",
  "author": "Sang Tran <t@sang.jp>",
  "license": "MIT",
  "packageManager": "yarn@3.6.1",
  "files": [
    "cjs",
    "esm"
